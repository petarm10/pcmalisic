{
    "name": "petar/malisic",
    "description": "Malisic WordPress Theme",
    "keywords": [
        "WordPress",
        "Theme",
        "Malisic"
    ],
    "license": "GPL-3.0",
    "version": "1.0.0",
    "require": {
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^1.0.0.0",
        "phpcompatibility/phpcompatibility-wp": "^2.0.0",
        "wp-coding-standards/wpcs": "^2.0.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "Mild\\": "./inc"
        },
        "files": [
            "./inc/Helpers.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
